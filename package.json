{
  "name": "@andrskr/eslint-config",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.9",
  "description": "Personal ESLint configuration",
  "author": {
    "name": "Andrii Skrebets",
    "email": "andriiskrebets@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrskr/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint --config ./index.js --report-unused-disable-directives --max-warnings 0",
    "test": "pnpm run lint .",
    "test:fix": "pnpm run lint --fix ."
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "prettier": "@andrskr/prettier-config",
  "peerDependencies": {
    "eslint": ">= 8.45.0",
    "typescript": ">= 5.1.6"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@rushstack/eslint-patch": "1.5.1",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-jsonc": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-markdown": "3.0.1",
    "eslint-plugin-mdx": "2.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-testing-library": "6.0.2",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-vitest": "0.3.2",
    "eslint-plugin-yml": "1.9.0",
    "jsonc-eslint-parser": "2.3.0",
    "yaml-eslint-parser": "1.2.2"
  },
  "devDependencies": {
    "@andrskr/prettier-config": "0.0.3",
    "@types/eslint": "8.44.3",
    "eslint": "8.50.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
